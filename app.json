{
  "expo": {
    "name": "tippmaschine",
    "slug": "tippmaschine",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ios.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fca5a5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/ios2.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-fg.png",
        "backgroundColor": "#fca5a5",
        "backgroundImage": "./assets/images/android-bg.png"
      },
      "icon": "./assets/images/android.png"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["sentry-expo"]
  }
}
